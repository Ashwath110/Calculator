# Copy to .env and edit before running
FLASK_ENV=production
SECRET_KEY=replace_this_with_a_secure_random_string
DATABASE_URL=sqlite:///calculator.db

# Optional: enable SSL for local dev (you can set to false)
USE_SSL=false
SSL_CERT_PATH=cert.pem
SSL_KEY_PATH=key.pem

# Optional: Google reCAPTCHA v2 keys (leave empty to disable)
RECAPTCHA_SITE_KEY=
RECAPTCHA_SECRET_KEY=

# Docker settings
PORT=8000
